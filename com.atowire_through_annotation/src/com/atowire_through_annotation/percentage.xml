<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- Define your beans here -->
   <context:annotation-config />
   <bean class="com.atowire_through_annotation.Mark" name="temp">
   <property name="sub1_marks" value="88" />
   <property name="sub2_marks" value="77" />
   <property name="sub3_marks" value="49" />
   <property name="sub4_marks" value="78" />
   <property name="sub5_marks" value="85" />
   <property name="sub6_marks" value="65" />
   </bean>
   
   <bean class="com.atowire_through_annotation.Mark" name="temp1">
   <property name="sub1_marks" value="89" />
   <property name="sub2_marks" value="77" />
   <property name="sub3_marks" value="45" />
   <property name="sub4_marks" value="65" />
   <property name="sub5_marks" value="70" />
   <property name="sub6_marks" value="71" />
   </bean>
   
   
   <bean class="com.atowire_through_annotation.Percentage" name="stud">
   <property name="student_name" value="karan pawar" />
   <property name="std" value="8th std" />
   <property name="sub1" value="Marathi" />
   <property name="sub2" value="Hindi" />
   <property name="sub3" value="English" />
   <property name="sub4" value="Math" />
   <property name="sub5" value="Science" />
   <property name="sub6" value="Social Science" />
   </bean>
   
 
    
   
</beans>





